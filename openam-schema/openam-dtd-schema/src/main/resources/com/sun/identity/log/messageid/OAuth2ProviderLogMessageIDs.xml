<!DOCTYPE logmessages
        PUBLIC "OpenSSO Log Message DTD/EN"
        "jar://com/sun/identity/log/messageid/amLogMessage.dtd"
        >

<logmessages prefix="OAuth2Provider">
    <!-- Token operations -->
    <logmessage name="CREATED_TOKEN" id="1"
                loglevel="LL_INFO"
                description="Created an oauth 2.0 token">
        <datainfo>
            <item>message</item>
            <item>token info</item>
        </datainfo>
        <triggers>
            <item>Created a new oauth 2.0 token</item>
        </triggers>
        <actions />
    </logmessage>
    <logmessage name="DELETED_TOKEN" id="2"
