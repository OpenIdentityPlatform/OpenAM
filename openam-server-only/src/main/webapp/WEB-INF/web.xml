            <param-name>agentsAuthzConfigurator</param-name>
            <param-value>org.forgerock.openam.authz.filter.PassThroughAuthorizationConfigurator</param-value>
        </init-param>
    </filter>

    <!-- filter declaration -->

    <filter-mapping>
        <filter-name>amSetupFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>XUIFilter</filter-name>
        <url-pattern>/UI/*</url-pattern>
        <dispatcher>FORWARD</dispatcher>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <filter-mapping>
        <filter-name>XUIFilter</filter-name>
        <url-pattern>/idm/EndUser</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>ResponseValidationFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>AuthNFilter</filter-name>
        <url-pattern>/ws/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>AuthZFilter</filter-name>
        <url-pattern>/ws/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CommonsAuthenticationFilter</filter-name>
        <url-pattern>/json/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CommonsAuthnContextMapperFilter</filter-name>
        <url-pattern>/json/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>RestAuthorizationDispatcherFilter</filter-name>
        <url-pattern>/json/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>SessionAuthorizationFilter</filter-name>
        <url-pattern>/json/sessions/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>AdminOnlyFilter</filter-name>
        <url-pattern>/json/tokens/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>PassThroughAuthorizationFilter</filter-name>
        <url-pattern>/json/dashboard/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>PassThroughAuthorizationFilter</filter-name>
        <url-pattern>/json/serverinfo/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CommonsAuthenticationFilter</filter-name>
        <url-pattern>/frrest/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CommonsAuthnContextMapperFilter</filter-name>
        <url-pattern>/frrest/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>AdminOnlyFilter</filter-name>
        <url-pattern>/frrest/oauth2/client/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>PassThroughAuthorizationFilter</filter-name>
        <url-pattern>/frrest/oauth2/token/*</url-pattern>
    </filter-mapping>

