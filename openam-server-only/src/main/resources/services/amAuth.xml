                                 type="single"
                                 syntax="boolean"
                                 i18nKey="a154"
                                 resourceName="xuiInterfaceEnabled">
                    <DefaultValues>
                        <Value>true</Value>
                    </DefaultValues>
                </AttributeSchema>
            </Global>
