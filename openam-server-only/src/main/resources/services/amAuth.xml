                                 type="single"
                                 syntax="boolean"
                                 i18nKey="a144"
                                 resourceName="storeInvalidAttemptsInDataStore">
                    <BooleanValues>
                        <BooleanTrueValue i18nKey="i18nTrue">true</BooleanTrueValue>
                        <BooleanFalseValue i18nKey="i18nFalse">false</BooleanFalseValue>
                    </BooleanValues>
                    <DefaultValues>
                        <Value>true</Value>
                    </DefaultValues>
                </AttributeSchema>
