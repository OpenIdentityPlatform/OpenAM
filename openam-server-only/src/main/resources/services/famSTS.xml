<!DOCTYPE ServicesConfiguration
    PUBLIC "=//iPlanet//Service Management Services (SMS) 1.0 DTD//EN"
    "jar://com/sun/identity/sm/sms.dtd">

<ServicesConfiguration>
    <Service name="sunFAMSTSService" version="1.0">
        <Schema 
            serviceHierarchy=""
            i18nFileName="famSTS"
            revisionNumber="10"
            i18nKey="sunSTS"
            resourceName="sts"
            propertiesViewBeanURL="../service/SecurityTokenService">
            <Global>
               <AttributeSchema name="stsIssuer"
                   type="single"
                   syntax="string"
                   i18nKey="a101"
                   resourceName="issuer">
                   <DefaultValues>
                       <Value>SunSTS</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema name="stsEndPoint"
                   type="single"
                   syntax="string"
                   i18nKey="a102"
                   resourceName="endpoint">
                   <DefaultValues>
                       <Value>@SERVER_PROTO@://@SERVER_HOST@:@SERVER_PORT@/@SERVER_URI@/sts</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema name="stsEncryptIssuedKey"
                   type="single"
                   syntax="boolean"
                   i18nKey="">
                   <DefaultValues>
                       <Value>true</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema name="stsEncryptIssuedToken"
                   type="single"
                   syntax="boolean"
                   i18nKey="">
                   <DefaultValues>
                       <Value>false</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema name="stsLifetime"
                   type="single"
                   syntax="number"
                   i18nKey="a105"
                   resourceName="lifetime">
                   <DefaultValues>
                       <Value>300000</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema name="stsTokenImplClass"
                   type="single"
                   syntax="string"
                   i18nKey="">
                   <DefaultValues>
                       <Value>com.sun.xml.ws.security.trust.impl.WSTrustContractImpl</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema name="stsCertAlias"
                   type="single"
                   syntax="string"
                   i18nKey="a107"
                   resourceName="certificateAlias">
                   <DefaultValues>
                       <Value>test</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema name="com.sun.identity.wss.sts.clientusertoken"
                   type="single"
                   syntax="string"
                   i18nKey="a108"
                   resourceName="clientUserTokenClass">
                   <DefaultValues>
                       <Value>com.sun.identity.wss.sts.STSClientUserToken</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                   name="SecurityMech"
                   type="multiple_choice"
                   syntax="string"
                   i18nKey="a109"
                   resourceName="securityMechanism">
                   <ChoiceValues>
                       <ChoiceValuesClassName
                           className="com.sun.identity.wss.security.ConfiguredWSPSecurityMech" />
                       </ChoiceValues>
                   <DefaultValues>
                       <Value>urn:sun:wss:security:null:X509Token</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                   name="isRequestSign"
                   type="single"
                   syntax="boolean"
                   i18nKey="a110"
                   resourceName="isRequestSignatureVerified">
                   <BooleanValues>
                       <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                       <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                   </BooleanValues>
                   <DefaultValues>
                       <Value>true</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                   name="isRequestHeaderEncrypt"
                   type="single"
                   syntax="boolean"
                   i18nKey="a111"
                   resourceName="isRequestHeaderDecrypted">
                   <BooleanValues>
                       <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                       <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                   </BooleanValues>
                   <DefaultValues>
                       <Value>false</Value>
                   </DefaultValues>
               </AttributeSchema> 
               <AttributeSchema
                   name="isRequestEncrypt"
                   type="single"
                   syntax="boolean"
                   i18nKey="a112"
                   resourceName="isRequestDecrypted">
                   <BooleanValues>
                       <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                       <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                   </BooleanValues>
                   <DefaultValues>
                       <Value>false</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                   name="isResponseSign"
                   type="single"
                   syntax="boolean"
                   i18nKey="a113"
                   resourceName="isResponseSigned">
                   <BooleanValues>
                       <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                       <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                   </BooleanValues>
                   <DefaultValues>
                       <Value>false</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                   name="isResponseEncrypt"
                   type="single"
                   syntax="boolean"
                   i18nKey="a114"
                   resourceName="isResponseEncrypted">
                   <BooleanValues>
                       <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                       <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                   </BooleanValues>
                   <DefaultValues>
                       <Value>false</Value>
                   </DefaultValues>
               </AttributeSchema>     
               <AttributeSchema
                   name="privateKeyAlias"
                   type="single"
                   syntax="string"
                   i18nKey="a115">
                   <DefaultValues>
                       <Value>test</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                   name="privateKeyType"
                   type="single"
                   syntax="string"
                   i18nKey="a116">
                   <DefaultValues>
                       <Value>PublicKey</Value>
                   </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                   name="publicKeyAlias"
                   type="single"
                   syntax="string"
                   i18nKey="a117">
                   <DefaultValues>
                       <Value>test</Value>
                   </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                    name="SigningRefType"
                        type="single_choice"
                        syntax="string"
                        i18nKey="a118"
                        resourceName="signingReferenceType">
                        <ChoiceValues>
                             <ChoiceValue i18nKey="a201">DirectReference</ChoiceValue>
                             <ChoiceValue i18nKey="a202">KeyIdentifierRef</ChoiceValue>
                             <ChoiceValue i18nKey="a203">X509IssuerSerialRef</ChoiceValue>
                        </ChoiceValues>
                        <DefaultValues>
                             <Value>DirectReference</Value>
                        </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                     name="EncryptionAlgorithm"
                     type="single_choice"
                     syntax="string"
                     i18nKey="a119"
                     resourceName="encryptionAlgorithm">
                         <ChoiceValues>
                             <ChoiceValue i18nKey="a204">DESede</ChoiceValue>
                             <ChoiceValue i18nKey="a205">AES</ChoiceValue>
                         </ChoiceValues>
                         <DefaultValues>
                             <Value>AES</Value>
                         </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                     name="EncryptionStrength"
                     type="single_choice"
                     syntax="number"
                     i18nKey="a120"
                     resourceName="encryptionStrength">
                     <ChoiceValues>
                         <ChoiceValue>0</ChoiceValue>
                         <ChoiceValue>112</ChoiceValue>
                         <ChoiceValue>128</ChoiceValue>
                         <ChoiceValue>168</ChoiceValue>
                         <ChoiceValue>192</ChoiceValue>
                         <ChoiceValue>256</ChoiceValue>
                     </ChoiceValues>
                     <DefaultValues>
                         <Value>128</Value>
                     </DefaultValues>
               </AttributeSchema>


               <AttributeSchema name="trustedIssuers"
                   type="list"
                   syntax="string"
                   i18nKey="a121"
                   resourceName="trustedIssuers">
                   <DefaultValues>
                       <Value>test:SunSTS</Value>
                   </DefaultValues>
               </AttributeSchema>

               <AttributeSchema name="trustedIPAddresses"
                   type="list"
                   syntax="string"
                   i18nKey="a122"
                   resourceName="trustedIpAddresses">
               </AttributeSchema>

               <AttributeSchema
                   name="UserCredential"
                   type="single"
                   syntax="string"
                   i18nKey="a123"
                   resourceName="userCredential">
                   <DefaultValues>
                       <Value>UserName:test|UserPassword:test</Value>
                   </DefaultValues>
               </AttributeSchema>
                    
               <AttributeSchema name="KerberosDomainServer"
                   type="single"
                   syntax="string"
                   i18nKey="a124"
                   resourceName="kerberosDomainServer">
               </AttributeSchema>

               <AttributeSchema
                    name="KerberosDomain"
                    type="single"
                    syntax="string"
                    i18nKey="a125"
                    resourceName="kerberosDomain">
               </AttributeSchema>

               <AttributeSchema
                    name="KerberosServicePrincipal"
                    type="single"
                    syntax="string"
                    i18nKey="a126"
                    resourceName="kerberosServicePrincipal">
               </AttributeSchema>

               <AttributeSchema
                    name="KerberosKeyTabFile"
                    type="single"
                    syntax="string"
                    i18nKey="a127"
                    resourceName="kerberosKeyTabFile">
               </AttributeSchema>

               <AttributeSchema
                    name="isVerifyKrbSignature"
                    type="single"
                    syntax="boolean"
                    i18nKey="a128"
                    resourceName="isVerifyKerberosSignature">
                    <BooleanValues>
                         <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                         <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                    </BooleanValues>
                    <DefaultValues>
                         <Value>false</Value>
                    </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                     name="SAMLAttributeMapping"
                     type="list"
                     syntax="string"
                     i18nKey="a129"
                     resourceName="samlAttributeMapping">
               </AttributeSchema>

               <AttributeSchema
                     name="NameIDMapper"
                     type="single"
                     syntax="string"
                     i18nKey="a130"
                     resourceName="nameIdMapper">
               </AttributeSchema>

               <AttributeSchema
                     name="AttributeNamespace"
                     type="single"
                     syntax="string"
                     i18nKey="a131"
                     resourceName="attributeNamespace">
                     <DefaultValues>
                         <Value>http://www.sun.com</Value>
                     </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                     name="includeMemberships"
                     type="single"
                     syntax="boolean"
                     i18nKey="a132">
                     <BooleanValues>
                         <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                         <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                     </BooleanValues>
                     <DefaultValues>
                         <Value>false</Value>
                     </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                     name="AuthenticationChain"
                     type="single_choice"
                     syntax="string"
                     i18nKey="a133"
                     resourceName="authenticationChain">
                     <ChoiceValues>
                         <ChoiceValuesClassName className="com.sun.identity.authentication.service.ConfiguredAuthServices" />
                     </ChoiceValues>
                     <DefaultValues>
                         <Value>[Empty]</Value>
                     </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                     name="DetectUserTokenReplay"
                     type="single"
                     syntax="boolean"
                     i18nKey="a134"
                     resourceName="detectUserTokenReplay">
                     <BooleanValues>
                         <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                         <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                     </BooleanValues>
                     <DefaultValues>
                         <Value>true</Value>
                     </DefaultValues>
               </AttributeSchema>

               <AttributeSchema
                     name="DetectMessageReplay"
                     type="single"
                     syntax="boolean"
                     i18nKey="a135"
                     resourceName="detectMessageReplay">
                     <BooleanValues>
                         <BooleanTrueValue i18nKey="a105">true</BooleanTrueValue>
                         <BooleanFalseValue i18nKey="a106">false</BooleanFalseValue>
                     </BooleanValues>
                     <DefaultValues>
                         <Value>true</Value>
                     </DefaultValues>
               </AttributeSchema>
               <AttributeSchema
                        name="SignedElements"
                        type="multiple_choice"
                        syntax="string"
                        i18nKey="a136"
                        resourceName="signedElements">
                        <ChoiceValues>
                            <ChoiceValuesClassName
                            className="com.sun.identity.wss.security.ConfiguredSignedElements" />
                        </ChoiceValues>
                        <DefaultValues>
                            <Value>Body</Value>
                            <Value>SecurityToken</Value>
                            <Value>Timestamp</Value>
                            <Value>To</Value>
                            <Value>From</Value>
                            <Value>ReplyTo</Value>
                            <Value>Action</Value>
                            <Value>MessageID</Value>
                        </DefaultValues>
                    </AttributeSchema>

            </Global>
        </Schema>
    </Service>
</ServicesConfiguration>
