<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.sun.identity</groupId>
	<artifactId>seraph-provider</artifactId>
	<version>1.0.0</version>
	<name>${pom.artifactId}</name>
	<description>OpenSSO authenticator for Atlassian JIRA and Confluence</description>
        <properties>
            <openam.version>snapshot9</openam.version>
        </properties>
	<dependencies>
		<dependency>
			<groupId>com.atlassian.seraph</groupId>
			<artifactId>atlassian-seraph</artifactId>
			<version>0.38</version>
		</dependency>
		<dependency>
			<groupId>com.atlassian.confluence</groupId>
			<artifactId>confluence</artifactId>
			<version>3.0.1</version>
		</dependency>
                <dependency>
			<groupId>com.atlassian.jira</groupId>
			<artifactId>atlassian-jira</artifactId>
			<version>4.0.1</version>
		</dependency>
               <dependency>
                    <groupId>com.forgerock.openam</groupId>
                    <artifactId>clientsdk</artifactId>
                    <version>${openam.version}</version>
                    <scope>provided</scope>
                </dependency>
		<dependency>
                    <groupId>javax.servlet</groupId>
                    <artifactId>servlet-api</artifactId>
                    <version>2.4</version>
		</dependency>
                <dependency>
                      <groupId>javax.activation</groupId>
                      <artifactId>activation</artifactId>
                      <version>1.1</version>
                </dependency>
                <dependency>
                      <groupId>javax.mail</groupId>
                      <artifactId>mail</artifactId>
                      <version>1.4</version>
                </dependency>
                <dependency>
                    <groupId>jta</groupId>
                    <artifactId>jta</artifactId>
                    <version>1.0.1</version>
                </dependency>
                <dependency>
                    <groupId>jndi</groupId>
                    <artifactId>jndi</artifactId>
                    <version>1.2.1</version>
                </dependency>
	</dependencies>
      <repositories>
            <repository>
                <id>atlassian-public</id>
                <url>https://maven.atlassian.com/repository/public</url>
                <snapshots><enabled>true</enabled></snapshots>
                <releases><enabled>true</enabled></releases>
            </repository>
            <repository>
                <id>forgerock</id>
                <url>http://repo.forgerock.org</url>
            </repository>
            <repository>
                <id>atlassian-m1</id>
                <url>https://maven.atlassian.com/content/groups/m1</url>
            </repository>
      </repositories>
</project>
